<view class="content"><view class="content"><image class="logo" src="{{avatar}}"></image><view class="info"><view class="appname">{{appName}}</view><view class="version">{{versionName}}</view></view><input class="input_css" placeholder="请输入账号" data-event-opts="{{[['input',[['__set_model',['','username','$event',[]]]]]]}}" value="{{username}}" bindinput="__e"/><input class="input_css" password="true" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/><input class="input_css" password="true" placeholder="请确认密码" data-event-opts="{{[['input',[['__set_model',['','confirmPassword','$event',[]]]]]]}}" value="{{confirmPassword}}" bindinput="__e"/><input hidden="{{!(isShowRegister)}}" class="input_css" type="text" placeholder="请输入邮箱" data-event-opts="{{[['input',[['__set_model',['','email','$event',[]]]]]]}}" value="{{email}}" bindinput="__e"/><block wx:if="{{error}}"><view class="error">{{error}}</view></block><view data-event-opts="{{[['tap',[['doLogin',['$event']]]]]}}" hidden="{{!(isShowLogin)}}" class="btn" bindtap="__e">{{loginBtnText}}</view><text data-event-opts="{{[['tap',[['showRegister',['$event']]]]]}}" hidden="{{!(isShowLogin)}}" class="text_class" bindtap="__e">没有账号？去注册!</text><view data-event-opts="{{[['tap',[['doRegister',['$event']]]]]}}" hidden="{{!(isShowRegister)}}" class="btn" bindtap="__e">{{registerBtnText}}</view><text data-event-opts="{{[['tap',[['showLogin',['$event']]]]]}}" hidden="{{!(isShowRegister)}}" class="text_class" bindtap="__e">已有账号？去登录!</text><view data-event-opts="{{[['tap',[['doBack',['$event']]]]]}}" class="btn_un" bindtap="__e">{{unLoginBtnText}}</view></view></view>